{{classLabel}}
<br/>
<table id="classList" class="unstyled">
{{#each classList}}}
<tr class="classItem" data-class-label="{{key}}">
<td style="text-align:right">
<span 
{{#if color}}
style="color: {{color}};"
{{/if}}
>{{key}}</span>
</td>
<td><span 
{{#if color}}
style="padding-right:{{value}}px;background: linear-gradient(to bottom right, #ffffff, {{color}});"
{{/if}}
>
{{value}}
</tr>
{{/each}}
</table>